<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.com.githublogin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.GitLoginApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.ListOfRepoActivity" />
        <!-- <activity android:name=".activities.LoginActivity"></activity> -->
         <activity android:name=".activities.UserProAndRepoActivity"></activity>
        <activity android:name=".activities.DetailsOfRepoActivity"></activity>
    </application>

</manifest>